{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "User ft. Google",
  "description": "The User",
  "required": [
   "username", 
   "email",
   "password"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The ID of the user."
    },
    "username": {
      "$id": "#/properties/username",
      "type": "string",
      "title": "The users username.",
      "examples": [
        "mike08"
      ]
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The users email"
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "The users password"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The creation date of the user"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated"
    }
  },
  "additionalProperties": false
}